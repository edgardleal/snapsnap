    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  }
