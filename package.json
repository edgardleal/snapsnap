    "lint:eslint": "eslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint:tslint": "tslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "npm run lint:tslint && npm run lint:eslint && npm run lint:md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salieri/jest-http-snapshot.git"
  },
